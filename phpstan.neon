parameters:
    level: 5
    paths:
        - app
        - src
    excludePaths:
        - app/Http/Resources/*
    ignoreErrors:
        -
            messages:
                - '#Call to an undefined method [a-zA-Z0-9\\_]+::allowedFilters\(\)#'
            paths:
                - src/Infrastructure/Persistence/Repositories/*
    treatPhpDocTypesAsCertain: false
